{
    "env": {
        "browser": true,
        "es2021": true,
        "node": true
    },
    "extends": [
        // "react-app",
        // "react-app/jest",
        "eslint:recommended"
        // "plugin:react/recommended",
        // "plugin:@typescript-eslint/recommended",
        // "plugin:react/jsx-runtime"
    ],
    "parserOptions": {
        "ecmaVersion": "latest",
        "sourceType": "module"
    },
    "rules": {
        "indent": ["error", 2],
        "linebreak-style": ["error", "unix"],
        "quotes": ["error", "double"],
        "semi": ["error", "always"],
        "no-unused-vars": ["warn"]
    }
}
